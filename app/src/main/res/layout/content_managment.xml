<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/content_main"
    android:background="@drawable/restaurant"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".ManagementActivity"
    tools:showIn="@layout/app_bar_management">

    <TextView
        android:visibility="gone"
        android:id="@+id/timeClock"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/ribeye_marrow"
        android:text="@string/tech_guru"
        android:textColor="#515151"
        android:layout_marginEnd="2dp"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:visibility="gone"
        android:id="@+id/timeClock2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="monospace"
        android:text="@string/tech_guru"
        android:layout_marginEnd="2dp"
        android:textColor="#515151"
        android:textSize="12dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/timeClock" />












    <TextView
        android:visibility="gone"
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/aclonica"
        android:text="@string/tech_guru"
        android:textColor="#0aa36d"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</android.support.constraint.ConstraintLayout>